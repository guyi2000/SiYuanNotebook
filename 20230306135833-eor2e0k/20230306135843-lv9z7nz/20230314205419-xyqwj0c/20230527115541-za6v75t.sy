				{
					"Type": "NodeCodeBlockCode",
					"Data": "import os\nimport urllib.parse\n\nfrom dotenv import load_dotenv\nfrom flask import Flask, jsonify, render_template\n\nfrom sqlalchemy import create_engine\nfrom sqlalchemy.orm import sessionmaker\nfrom sqlalchemy.ext.automap import automap_base\n\nfrom coordinate import wgs84_to_bd09\n\n# 读入 `.env` 文件中的环境变量\nload_dotenv()\n# 从环境变量中读取密码，更加安全\npassword = os.environ.get(\"DATABASE_PASSWORD\", \"\")\n# 由于密码中有特殊字符，进行编码\nencoded_password = urllib.parse.quote_plus(password)\n\n# 建立连接\nengine = create_engine(\n    f\"postgresql://postgres:{encoded_password}@192.168.0.103:5432/tsdb\"\n)\nbase = automap_base()\n# automap，自动获取表格的类型\nbase.prepare(engine, reflect=True)\nSession = sessionmaker(bind=engine)\nsession = Session()\n\n# 读取 OBU 表\nOBU = base.classes.obu\n\napp = Flask(__name__)\n\n\n@app.route(\"/\")\ndef index():\n    \"\"\"首页\"\"\"\n    return render_template(\"./baidu.html\")\n\n\n@app.route(\"/get_data\", methods=[\"GET\"])\ndef get_data():\n    \"\"\"获取数据，内部对数据进行坐标转化，从 WGS84 坐标系到 BD09 坐标系\"\"\"\n    data = session.query(OBU) \\\n                  .filter(OBU.vehicleid == \"MK000000000000007\") \\\n                  .limit(10000).all()\n    translate_data = []\n    for item in data:\n        lat = float(item.latitude)\n        lon = float(item.longitude)\n        lon, lat = wgs84_to_bd09(lon, lat)\n        translate_data.append({\n            \"timestampgnss\": item.timestampgnss,\n            \"lat\": lat,\n            \"lon\": lon,\n        })\n\n    return jsonify(translate_data)\n\n\nif __name__ == \"__main__\":\n    app.run(port=8888)\n",
